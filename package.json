{
  "name": "Finder",
  "version": "0.0.5",
  "description": "A study app",
  "main": "renderer.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package": "node package_python.js && npm run -s package-electron && npm run -s package-cleanup",
    "package1": "npm run -s package-electron && npm run -s package-cleanup",
    "package-electron": "electron-builder --win --publish=always",
    "package-cleanup": "echo Cleaning Up... && rd /s /q build && rd /s /q dist-python"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "asturgeon123",
        "repo": "quizlet_bot"
      }
    ],
    "appId": "com.electron-flask.my-app",
    "productName": "Finder",
    "asar": false,
    "asarUnpack": [
      "**/*.node"
    ],
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "renderer.js",
      "icon.png",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "dist-python/",
        "to": "app/dist-python",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "electron-updater": "^4.6.5",
    "ps-tree": "^1.2.0"
  },
  "devDependencies": {
    "electron": "^13.6.0",
    "electron-builder": "^22.11.7"
  }
}
